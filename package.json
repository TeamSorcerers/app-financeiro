{
  "name": "app-financeiro",
  "version": "0.1.0",
  "description": "Aplicação web moderna para controle de despesas pessoais e colaborativas",
  "author": "Eduardo Gimenez, Gabriel Costa, Gabriel Nicolas, Lucas Theodoro, Raquel Nunes",
  "license": "MIT",
  "homepage": "https://github.com/TeamSorcerers/app-financeiro#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/TeamSorcerers/app-financeiro.git"
  },
  "bugs": {
    "url": "https://github.com/TeamSorcerers/app-financeiro/issues"
  },
  "keywords": [
    "finance",
    "expense-tracker",
    "collaborative",
    "nextjs",
    "typescript",
    "prisma",
    "real-time"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env APP_ENV=development tsx server.ts",
    "start": "cross-env APP_ENV=production tsx server.ts",
    "build": "next build --turbopack",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.4"
  },
  "packageManager": "npm@10.2.4",
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "bcryptjs": "^3.0.2",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "next-auth": "^5.0.0-beta.29",
    "pino": "^9.13.1",
    "pino-pretty": "^13.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "socket.io-client": "^4.8.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@eslint/eslintrc": "^3",
    "@phoenix35/eslint-config": "^1.8.0",
    "@tailwindcss/postcss": "4.1.13",
    "@types/node": "24.3.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prisma": "^6.15.0",
    "socket.io": "^4.8.1",
    "tailwindcss": "4.1.13",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "typescript-eslint": "^8.42.0"
  }
}
